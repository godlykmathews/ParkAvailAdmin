{
    "version": 2,
    "builds": [
      {
        "src": "bin/www",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "bin/www"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }